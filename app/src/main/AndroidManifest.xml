<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.getcleaner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cleaner_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"

        android:theme="@style/Theme.GetCleaner">


        <activity
            android:name=".activities.activity_start_menu"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.activity_sign_cleaner"
            android:exported="false"/>
        <activity android:name=".activities.activity_manage_my_account"
            android:exported="false"/>
        <activity android:name=".activities.activity_sign_searcher"
            android:exported="false"/>
        <activity android:name=".activities.activity_cleaners_list"
            android:exported="false"/>
        <activity android:name=".activities.activity_menu_find_cleaner"
            android:exported="false"/>

        <activity android:name=".activities.activity_comments_rates"
            android:exported="false"/>
    </application>

</manifest>